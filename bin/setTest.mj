program TestAddMethod

set s1, s2, s3;
{
    void main() {
		
		// Your spec guarantees the destination is pre-allocated and large enough.
        s1 = new set[10];
        
        s2 = new set[5];
        add(s2, 10);
        add(s2, 20);

        s3 = new set[5];
        add(s3, 30);
        add(s3, 10); // duplicate element

        // Perform the union and assign the result to s1
        s1 = s2 union s3;
        
    }
}